{
  "name": "sejator/spotify",
  "description": "Aplikasi pemutar musik dari spotify ditambah fitur jadwal sholat, play backsound dan iklan ",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Sejator Dev",
      "email": "sejatordev@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "codeigniter4/framework": "4.4.7",
    "jwilsson/spotify-web-api-php": "^5.7"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9",
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "^9.1"
  },
  "autoload": {
    "exclude-from-classmap": [
      "**/Database/Migrations/**"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Support\\": "tests/_support"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit"
  }
}
